<!doctype html>
<html lang="en">

<head>
    <title>CarRental | Ana Sayfa</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@400;700;900&display=swap" rel="stylesheet">

    <link rel="stylesheet" href="/thema/fonts/icomoon/style.css">

    <link rel="stylesheet" href="/thema/css/bootstrap.min.css">
    <link rel="stylesheet" href="/thema/css/bootstrap-datepicker.css">
    <link rel="stylesheet" href="/thema/css/jquery.fancybox.min.css">
    <link rel="stylesheet" href="/thema/css/owl.carousel.min.css">
    <link rel="stylesheet" href="/thema/css/owl.theme.default.min.css">
    <link rel="stylesheet" href="/thema/fonts/flaticon/font/flaticon.css">
    <link rel="stylesheet" href="/thema/css/aos.css">

    <!-- MAIN CSS -->
    <link rel="stylesheet" href="/thema/css/style.css">

</head>

<body>

    <div class="site-wrap" id="home-section">

        <div class="site-mobile-menu site-navbar-target">
            <div class="site-mobile-menu-header">
                <div class="site-mobile-menu-close mt-3">
                    <span class="icon-close2 js-menu-toggle"></span>
                </div>
            </div>
            <div class="site-mobile-menu-body"></div>
        </div>

        <header class="site-navbar site-navbar-target" role="banner">
            <div class="container">
                <div class="row align-items-center position-relative">
                    <div class="col-3">
                        <div class="site-logo">
                            <a href="#"><strong>CarRental</strong></a>
                        </div>
                    </div>
                    <div class="col-9  text-right">
                        <span class="d-inline-block d-lg-none">
                            <a href="#" class=" site-menu-toggle js-menu-toggle py-5 "><span class="icon-menu h3 text-black"></span>
                            </a>
                        </span>
                        <nav class="site-navigation text-right ml-auto d-none d-lg-block" role="navigation">
                            <ul class="site-menu main-menu js-clone-nav ml-auto ">
                            <li class="active"><a asp-controller="Home" asp-action="Index" class="nav-link">Ana Sayfa</a></li>
                            <li><a asp-controller="Listing" asp-action="Index" class="nav-link">Araç Listesi</a></li>
                            <li><a href="#" class="nav-link">Yorumlar</a></li>
                            <li><a href="#" class="nav-link">Blog</a></li>
                            <li><a href="#" class="nav-link">Hakkımızda</a></li>
                            <li><a href="#" class="nav-link">İletişim</a></li>
                            <li><a asp-controller="Car" asp-action="CarList" class="nav-link">Admin</a></li>
                            </ul>
                        </nav>
                    </div>
                </div>
            </div>
        </header>

        @RenderBody()

    </div>

    <script src="/thema/js/jquery-3.3.1.min.js"></script>
    <script src="/thema/js/popper.min.js"></script>
    <script src="/thema/js/bootstrap.min.js"></script>
    <script src="/thema/js/owl.carousel.min.js"></script>
    <script src="/thema/js/jquery.sticky.js"></script>
    <script src="/thema/js/jquery.waypoints.min.js"></script>
    <script src="/thema/js/jquery.animateNumber.min.js"></script>
    <script src="/thema/js/jquery.fancybox.min.js"></script>
    <script src="/thema/js/jquery.easing.1.3.js"></script>
    <script src="/thema/js/bootstrap-datepicker.min.js"></script>
    <script src="/thema/js/aos.js"></script>

    <script src="js/main.js"></script>

</body>

</html>

